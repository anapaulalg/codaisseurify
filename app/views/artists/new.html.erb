<h1>New Artist</h1>
<h3>Add your favorite Artists!</h3>

<%= form_for @artist do |f| %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit "Add Artist" %>
<% end %>
